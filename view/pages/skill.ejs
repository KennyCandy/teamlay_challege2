<div class="tab-pane fade" id="skill">
    <form class="panel-group form-horizontal" id="validation_form_skill">
        <div class="panel panel-info">
            <div class="panel-body">
                <div class="skillname col-sm-3 col-lg-3">
                    <img  src="/img/default_avatar.jpg" class="default_avatar" />
                    <div class="form-group row">
                        <input type="text" name="nameskill" class="form-control valid" placeholder="Enter your skill"/>
                    </div>
                </div>
                <div class="skillinfo col-sm-9 col-lg-9">
                    <div class="content-skill col-sm-12 col-lg-12 row">
                        <div class="form-group row">
                            <label class="col-sm-3 col-lg-3 control-label">Expertise</label>
                            <div class="col-sm-9 col-lg-9 controls">
                                <div id="stars-default" data-rating="1"></div>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-lg-3 control-label">Experience</label>
                            <div class="col-sm-5 col-lg-5 controls">
                            <select class="form-control">
                                <option value="0 month">0 month</option>
                                <option value="1-6 months">1-6 months</option>
                                <option value="6-12 months">6-12 months</option>
                                <option value="1-2 years">1-2 years</option>
                                <option value="2-5 years">2-5 years</option>
                            </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-sm-3 col-lg-3 control-label">Last year used</label>
                            <div class="col-sm-5 col-lg-5 controls">
                            <select class="form-control" id="skillyear">
                            
                            </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div >
                    <label class="col-sm-3 col-lg-3 control-label">&nbsp;</label>
                    <div class="col-sm-9 col-lg-9 controls">        
                        <button type="button" id="btnAddListExp"  class="btn btn-success"><i class="fa fa-plus">&nbsp;Add To List</i></button>
                        <button type="button" id="btnSaveExp" class="btn btn-success"><i class="fa fa-save">&nbsp;Save</i></button>
                    </div>               
                </div>
            </div>
        </div>   
    </form>
    
    <div class="form-group row">
            <div class="box">
                <div class="box-title">
                <h4> List of Skills</h4>
                </div>
                <div class="box-content">
                    <div class="table-big">
                        <table class="table table-striped table-hover fill-head" id="list-skill">
                            <thead>
                                <tr style="font-size: 13px">
                                    <th>#</th>
                                    <th>Skill</th>
                                    <th>Expertise</th>
                                    <th>Experience</th>
                                    <th>Last year used</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>  
                                  <tr style='font-size:13px'>
                                    <td>
                                        1
                                    </td>
                                    <td>
                                        <img  src="/img/default_avatar.jpg" class="default_avatar" />
                                        <div class="form-group row">
                                            skill name
                                        </div>
                                    </td>
                                    <td>
                                        <div class="stars-default1" data-rating="1"></div>
                                        <div class="stars-default2" data-rating="2"></div>
                                        <div class="stars-default3" data-rating="3"></div>
                                    </td>
                                  </tr>                          
                            </tbody>
                        </table>
                    </div>
                </div>         
                    
            </div>            
         </div>
    
</div>
<script type="text/javascript" src="js/skill.js"> </script>
